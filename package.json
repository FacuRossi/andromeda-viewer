{
  "name": "andromeda-viewer",
  "version": "0.0.1",
  "description": "Small web-client for connecting to the virtual world of Second Life.",
  "scripts": {
    "prebuild": "npm run build-messages",
    "build": "react-scripts build",
    "prewatch": "npm run build-messages",
    "watch": "react-scripts start",
    "help": "markdown-chalk",
    "pretest": "npm run build-messages",
    "test": "standard && react-scripts test --env=jsdom",
    "fixStyle": "standard --fix",
    "build-messages": "node ./tools/createMessageTemplate",
    "update-coc": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'",
    "startDev": "concurrently \"hoodie --inMemory --public build\" \"npm run watch\"",
    "start": "hoodie --public build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Terreii/andromeda-viewer.git"
  },
  "author": {
    "name": "Christopher Astfalk",
    "email": "christopher.astfalk@icloud.com",
    "url": "http://christopher-astfalk.de/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.0"
  },
  "bugs": {
    "url": "https://github.com/Terreii/andromeda-viewer/issues"
  },
  "homepage": "https://github.com/Terreii/andromeda-viewer",
  "proxy": "http://localhost:8080",
  "devDependencies": {
    "@hoodie/client": "^10.1.1",
    "concurrently": "^3.5.1",
    "immutable": "^3.8.2",
    "markdown-chalk": "^1.4.0",
    "pouchdb": "^6.4.3",
    "prop-types": "^15.6.1",
    "rc-tabs": "^9.2.5",
    "react": "^16.4.1",
    "react-burger-menu": "^2.5.2",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-burger-menu": "^0.2.5",
    "redux-thunk": "^2.3.0",
    "standard": "^10.0.3",
    "styled-components": "^2.4.1",
    "weallbehave": "^1.2.0",
    "weallcontribute": "^1.0.8"
  },
  "dependencies": {
    "hoodie": "^28.2.6",
    "macaddress": "^0.2.8",
    "node-fetch": "^2.1.2",
    "uuid": "^3.2.1",
    "ws": "^3.3.3",
    "xmlrpc": "^1.3.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest",
      "es6"
    ]
  }
}
