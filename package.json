{
  "name": "andromeda-viewer",
  "version": "0.0.1",
  "description": "Small web-client for connecting to the virtual world of Second Life.",
  "main": "server.js",
  "scripts": {
    "build": "node ./tools/createMessageTemplate.js && browserify -p [ css-modulesify -o builds/main.css ] main.js -o builds/bundle.js",
    "watch": "node ./tools/createMessageTemplate.js && watchify -p [ css-modulesify -o builds/main.css ] main.js -o builds/bundle.js --debug --verbose",
    "help": "markdown-chalk",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "test": "standard && mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Terreii/andromeda-viewer.git"
  },
  "author": {
    "name": "Christopher Astfalk",
    "email": "christopher.astfalk@icloud.com",
    "url": "http://christopher-astfalk.de/"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4.2.4"
  },
  "bugs": {
    "url": "https://github.com/Terreii/andromeda-viewer/issues"
  },
  "homepage": "https://github.com/Terreii/andromeda-viewer",
  "devDependencies": {
    "browserify": "^13.0.0",
    "watchify": "^3.7.0",
    "css-modulesify": "^0.17.0",
    "flux": "^2.1.1",
    "immutable": "^3.7.4",
    "markdown-chalk": "^1.4.0",
    "mocha": "^2.2.5",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-tabs": "^0.5.0",
    "reactify": "^1.1.1",
    "standard": "^6.0.1"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "express": "^4.13.3",
    "express-ws": "^1.0.0",
    "macaddress": "^0.2.8",
    "uuid": "^2.0.1",
    "xmlrpc": "^1.3.1"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
