{
  "name": "andromeda-viewer",
  "version": "0.0.1",
  "description": "Small web-client for connecting to the virtual world of Second Life.",
  "main": "main.js",
  "scripts": {
    "build": "webpack -p --progress --colors",
    "watch": "webpack -d --progress --colors --watch",
    "help": "markdown-chalk",
    "pretest": "webpack --config test.config.js",
    "test": "standard && mocha --compilers js:babel-core/register",
    "posttest": "rm public/testBundle.js",
    "fixStyle": "standard --fix",
    "update-coc": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'",
    "startDev": "hoodie --inMemory",
    "start": "hoodie"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Terreii/andromeda-viewer.git"
  },
  "author": {
    "name": "Christopher Astfalk",
    "email": "christopher.astfalk@icloud.com",
    "url": "http://christopher-astfalk.de/"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.9.0"
  },
  "bugs": {
    "url": "https://github.com/Terreii/andromeda-viewer/issues"
  },
  "homepage": "https://github.com/Terreii/andromeda-viewer",
  "devDependencies": {
    "@hoodie/client": "^10.1.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.28.7",
    "immutable": "^3.8.2",
    "json-loader": "^0.5.7",
    "markdown-chalk": "^1.4.0",
    "mocha": "^3.5.3",
    "pouchdb": "^6.3.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-tabs": "^1.1.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "standard": "^10.0.3",
    "style-loader": "^0.18.0",
    "styled-components": "^2.2.2",
    "weallbehave": "^1.2.0",
    "weallcontribute": "^1.0.8",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "hoodie": "^28.2.2",
    "macaddress": "^0.2.8",
    "uuid": "^3.1.0",
    "ws": "^3.2.0",
    "xmlrpc": "^1.3.2"
  },
  "babel": {
    "presets": [
      "latest",
      "react"
    ]
  }
}
